<!DOCTYPE component SYSTEM "../misc/component.dtd" [
<!ENTITY standard_uicomponent_attributes		SYSTEM "../entities/standard_uicomponent_attributes.xml">
<!ENTITY output_component_attributes			SYSTEM "../entities/output_component_attributes.xml">
<!ENTITY input_component_attributes				SYSTEM "../entities/input_component_attributes.xml">
<!ENTITY command_component_attributes			SYSTEM "../entities/command_component_attributes.xml">
<!ENTITY widget_var_attribute					SYSTEM "../entities/widget_var_attribute.xml">
<!ENTITY html_command_button_attributes			SYSTEM "../entities/html_command_button_attributes.xml">
]>
<component>
	<tag>button</tag>
	<tagClass>org.primefaces.ui.component.button.ButtonTag</tagClass>
	<componentClass>org.primefaces.ui.component.button.Button</componentClass>
	<componentType>org.primefaces.ui.Button</componentType>
	<componentFamily>org.primefaces.ui</componentFamily>
	<rendererType>org.primefaces.ui.component.ButtonRenderer</rendererType>
	<rendererClass>org.primefaces.ui.component.button.ButtonRenderer</rendererClass>
	<parent>javax.faces.component.UICommand</parent>
	<attributes>
		&command_component_attributes;
		&widget_var_attribute;
		<attribute>
			<name>type</name>
			<required>false</required>
			<type>java.lang.String</type>
			<defaultValue>submit</defaultValue>
			<description>
				submit|reset, Defines how the button is displayed and will work.
			</description>
		</attribute>
		<attribute>
			<name>async</name>
			<required>false</required>
			<type>java.lang.Boolean</type>
			<defaultValue>false</defaultValue>
			<description>
				Specifies the submit mode, when set to true, submit would be made with Ajax.
			</description>
		</attribute>
		<attribute>
			<name>update</name>
			<required>false</required>
			<type>java.lang.String</type>
			<description>
				Server side id of the component(s) to be updated after async partial submit request.
			</description>
		</attribute>
		<attribute>
			<name>oncomplete</name>
			<required>false</required>
			<type>java.lang.String</type>
			<description>
				Javascript event to be called when ajax request is completed.
			</description>
		</attribute>
		<attribute>
			<name>onblur</name>
			<required>false</required>
			<type>java.lang.String</type>
			<ignoreInComponent>true</ignoreInComponent>
			<description>
				 onblur dom event handler
			</description>
		</attribute>
		<attribute>
			<name>onchange</name>
			<required>false</required>
			<type>java.lang.String</type>
			<ignoreInComponent>true</ignoreInComponent>
			<description>
				 onchange dom event handler
			</description>
		</attribute>
		<attribute>
			<name>onclick</name>
			<required>false</required>
			<type>java.lang.String</type>
			<ignoreInComponent>true</ignoreInComponent>
			<description>
				 onclick dom event handler
			</description>
		</attribute>
		<attribute>
			<name>ondblclick</name>
			<required>false</required>
			<type>java.lang.String</type>
			<ignoreInComponent>true</ignoreInComponent>
			<description>
				 ondblclick dom event handler
			</description>
		</attribute>
		<attribute>
			<name>onfocus</name>
			<required>false</required>
			<type>java.lang.String</type>
			<ignoreInComponent>true</ignoreInComponent>
			<description>
				 onfocus dom event handler
			</description>
		</attribute>
		<attribute>
			<name>onkeydown</name>
			<required>false</required>
			<type>java.lang.String</type>
			<ignoreInComponent>true</ignoreInComponent>
			<description>
				 onkeydown dom event handler
			</description>
		</attribute>
		<attribute>
			<name>onkeypress</name>
			<required>false</required>
			<type>java.lang.String</type>
			<ignoreInComponent>true</ignoreInComponent>
			<description>
				 onkeypress dom event handler
			</description>
		</attribute>
		<attribute>
			<name>onkeyup</name>
			<required>false</required>
			<type>java.lang.String</type>
			<ignoreInComponent>true</ignoreInComponent>
			<description>
				 onkeyup dom event handler
			</description>
		</attribute>
		<attribute>
			<name>onmousedown</name>
			<required>false</required>
			<type>java.lang.String</type>
			<ignoreInComponent>true</ignoreInComponent>
			<description>
				 onmousedown dom event handler
			</description>
		</attribute>
		<attribute>
			<name>onmousemove</name>
			<required>false</required>
			<type>java.lang.String</type>
			<ignoreInComponent>true</ignoreInComponent>
			<description>
				 onmousemove dom event handler
			</description>
		</attribute>
		<attribute>
			<name>onmouseout</name>
			<required>false</required>
			<type>java.lang.String</type>
			<ignoreInComponent>true</ignoreInComponent>
			<description>
				 onmouseout dom event handler
			</description>
		</attribute>
		<attribute>
			<name>onmouseover</name>
			<required>false</required>
			<type>java.lang.String</type>
			<ignoreInComponent>true</ignoreInComponent>
			<description>
				 onmouseover dom event handler
			</description>
		</attribute>
		<attribute>
			<name>onmouseup</name>
			<required>false</required>
			<type>java.lang.String</type>
			<ignoreInComponent>true</ignoreInComponent>
			<description>
				 onmouseup dom event handler
			</description>
		</attribute>
		<attribute>
			<name>onselect</name>
			<required>false</required>
			<type>java.lang.String</type>
			<ignoreInComponent>true</ignoreInComponent>
			<description>
				 onselect dom event handler
			</description>
		</attribute>
	</attributes>
	<resources>
		<resource>
			<name>/yui/fonts/fonts-min.css</name>
		</resource>
		<resource>
			<name>/yui/container/assets/skins/sam/container.css</name>
		</resource>
		<resource>
			<name>/yui/button/assets/skins/sam/button.css</name>
		</resource>
		<resource>
			<name>/yui/utilities/utilities.js</name>
		</resource>
		<resource>
			<name>/yui/datasource/datasource-min.js</name>
		</resource>
		<resource>
			<name>/yui/container/container-min.js</name>
		</resource>
		<resource>
			<name>/yui/button/button-min.js</name>
		</resource>
		<resource>
			<name>/primefaces/ajax/ajax.js</name>
		</resource>
		<resource>
			<name>/prototype/prototype.js</name>
		</resource>
		<resource>
			<name>/yui/cookie/cookie-min.js</name>
		</resource>
	</resources>
</component>