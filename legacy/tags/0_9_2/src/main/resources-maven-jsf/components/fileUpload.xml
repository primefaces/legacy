<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE component SYSTEM "../misc/component.dtd" [
<!ENTITY standard_uicomponent_attributes		SYSTEM "../entities/standard_uicomponent_attributes.xml">
<!ENTITY output_component_attributes			SYSTEM "../entities/output_component_attributes.xml">
<!ENTITY input_component_attributes				SYSTEM "../entities/input_component_attributes.xml">
<!ENTITY html_input_textarea_attributes			SYSTEM "../entities/html_input_textarea_attributes.xml">
<!ENTITY widget_var_attribute					SYSTEM "../entities/widget_var_attribute.xml">
]>
<component>
	<tag>fileUpload</tag>
	<tagClass>org.primefaces.ui.component.fileupload.FileUploadTag</tagClass>
	<componentHandlerClass>org.primefaces.ui.component.fileupload.FileUploadHandler</componentHandlerClass>
	<componentClass>org.primefaces.ui.component.fileupload.FileUpload</componentClass>
	<componentType>org.primefaces.ui.FileUpload</componentType>
	<componentFamily>org.primefaces</componentFamily>
	<rendererType>org.primefaces.ui.component.FileUploadRenderer</rendererType>
	<rendererClass>org.primefaces.ui.component.fileupload.FileUploadRenderer</rendererClass>
	<parent>javax.faces.component.UIComponentBase</parent>
	<attributes>
		&standard_uicomponent_attributes;
		<attribute>
			<name>update</name>
			<required>false</required>
			<type>java.lang.String</type>
			<description>
				Client side id of the component(s) to be updated after file upload completes.
			</description>
		</attribute>
		<attribute>
			<name>fileUploadListener</name>
			<required>false</required>
			<type>javax.el.MethodExpression</type>
			<method-signature>void fileUploadListener(org.primefaces.ui.event.fileupload.FileUploadEvent)</method-signature>
			<description>
				Method expression to listen file upload events.
			</description>
		</attribute>
		<attribute>
			<name>multiple</name>
			<required>false</required>
			<type>java.lang.Boolean</type>
			<defaultValue>false</defaultValue>
			<description>
				Allows for multiple file uploads, default is set to false.
			</description>
		</attribute>
		<attribute>
			<name>style</name>
			<required>false</required>
			<type>java.lang.String</type>
			<description>
			 	Style of the element containing the fileupload controls
			</description>
		</attribute>
		<attribute>
			<name>styleClass</name>
			<required>false</required>
			<type>java.lang.String</type>
			<description>
			 	Style class of the element containing the fileupload controls
			</description>
		</attribute>
		<attribute>
			<name>browseStyle</name>
			<required>false</required>
			<type>java.lang.String</type>
			<description>
			 	Container element style for the browse trigger
			</description>
		</attribute>
		<attribute>
			<name>browseStyleClass</name>
			<required>false</required>
			<type>java.lang.String</type>
			<description>
				Container element style class for the browse trigger
			</description>
		</attribute>
		<attribute>
			<name>uploadStyle</name>
			<required>false</required>
			<type>java.lang.String</type>
			<description>
				Container element style for the upload trigger
			</description>
		</attribute>
		<attribute>
			<name>uploadStyleClass</name>
			<required>false</required>
			<type>java.lang.String</type>
			<description>
				Container element style class for the upload trigger
			</description>
		</attribute>
		<attribute>
			<name>clearStyle</name>
			<required>false</required>
			<type>java.lang.String</type>
			<description>
				Container element style for the clear trigger
			</description>
		</attribute>
		<attribute>
			<name>clearStyleClass</name>
			<required>false</required>
			<type>java.lang.String</type>
			<description>
				Container element style class for the clear trigger
			</description>
		</attribute>
	</attributes>
	<resources>
		<resource>
			<name>/yui/datatable/assets/skins/sam/datatable.css</name>
		</resource>
		<resource>
			<name>/yui/utilities/utilities.js</name>
		</resource>
		<resource>
			<name>/yui/cookie/cookie-min.js</name>
		</resource>
		<resource>
			<name>/yui/datasource/datasource-min.js</name>
		</resource>
		<resource>
			<name>/yui/datatable/datatable-min.js</name>
		</resource>
		<resource>
			<name>/jquery/jquery.js</name>
		</resource>
		<resource>
			<name>/primefaces/ajax/ajax.js</name>
		</resource>
		<resource>
			<name>/yui/uploader/uploader-min.js</name>
		</resource>
		<resource>
			<name>/primefaces/uploader/uploader.js</name>
		</resource>
	</resources>
</component>