<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE component SYSTEM "../misc/component.dtd" [
<!ENTITY standard_uicomponent_attributes		SYSTEM "../entities/standard_uicomponent_attributes.xml">
<!ENTITY widget_var_attribute					SYSTEM "../entities/widget_var_attribute.xml">
]>
<component>
	<tag>panel</tag>
	<tagClass>org.primefaces.ui.component.panel.PanelTag</tagClass>
	<componentClass>org.primefaces.ui.component.panel.Panel</componentClass>
	<componentType>org.primefaces.ui.Panel</componentType>
	<componentFamily>org.primefaces</componentFamily>
	<rendererType>org.primefaces.ui.component.PanelRenderer</rendererType>
	<rendererClass>org.primefaces.ui.component.panel.PanelRenderer</rendererClass>
	<parent>javax.faces.component.UIPanel</parent>
	<attributes>
		&standard_uicomponent_attributes;
		<attribute>
			<name>header</name>
			<required>false</required>
			<type>java.lang.String</type>
			<description>Header text of the panel</description>
		</attribute>
		<attribute>
			<name>footer</name>
			<required>false</required>
			<type>java.lang.String</type>
			<description>Footer text of the panel</description>
		</attribute>
		<attribute>
			<name>toggleable</name>
			<required>false</required>
			<type>java.lang.Boolean</type>
			<defaultValue>false</defaultValue>
			<description>Makes the panel toggleable, places a toggle icon for user interaction</description>
		</attribute>
		<attribute>
			<name>toggleSpeed</name>
			<required>false</required>
			<type>java.lang.Integer</type>
			<defaultValue>1000</defaultValue>
			<description>Speed of toggling in milliseconds. Default is 1000.</description>
		</attribute>
		<attribute>
			<name>style</name>
			<required>false</required>
			<type>java.lang.String</type>
			<description>Style of the main container of panel.</description>
		</attribute>
		<attribute>
			<name>styleClass</name>
			<required>false</required>
			<type>java.lang.String</type>
			<description>Style class of the main container of panel.</description>
		</attribute>
	</attributes>
	<resources>
		<resource>
			<name>/primefaces/panel/assets/panel.css</name>
		</resource>
		<resource>
			<name>/jquery/jquery.js</name>
		</resource>
		<resource>
			<name>/jquery/plugins/effects/effects.js</name>
		</resource>
	</resources>
</component>