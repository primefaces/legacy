<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE component SYSTEM "../misc/component.dtd" [
<!ENTITY standard_uicomponent_attributes		SYSTEM "../entities/standard_uicomponent_attributes.xml">
]>
<component>
	<tag>submenu</tag>
	<tagClass>org.primefaces.component.submenu.SubmenuTag</tagClass>
	<componentClass>org.primefaces.component.submenu.Submenu</componentClass>
	<componentType>org.primefaces.component.Submenu</componentType>
	<componentFamily>org.primefaces.component</componentFamily>
	<parent>javax.faces.component.UIComponentBase</parent>
	<attributes>
		&standard_uicomponent_attributes;
		<attribute>
			<name>label</name>
			<required>false</required>
			<type>java.lang.String</type>
			<description>Label of the submenu header</description>
		</attribute>
		<attribute>
			<name>url</name>
			<required>false</required>
			<type>java.lang.String</type>
			<description>URL that will be navigated to when the submenu title is selected</description>
		</attribute>
		<attribute>
			<name>style</name>
			<required>false</required>
			<type>java.lang.String</type>
			<description>Style of the submenu label</description>
		</attribute>
		<attribute>
			<name>styleClass</name>
			<required>false</required>
			<type>java.lang.String</type>
			<description>Style class of the submenu label</description>
		</attribute>
		<attribute>
			<name>onclick</name>
			<required>false</required>
			<type>java.lang.String</type>
			<description>onclick event for root submenus with no menuitems.</description>
		</attribute>
	</attributes>
</component>
