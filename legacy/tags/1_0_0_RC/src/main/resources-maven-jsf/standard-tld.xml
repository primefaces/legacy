	<tag>
		<name>dataExporter</name>
		<tag-class>org.primefaces.component.export.DataExporterTag</tag-class>
		<body-content>empty</body-content>
		<attribute>
			<description>Server side of the datatable to be exported</description>
			<name>target</name>
			<required>true</required>
			<deferred-value>
				<type>java.lang.String</type>
			</deferred-value>
		</attribute>
		<attribute>
			<description>Type of the format, could be xls, pdf, xls, xml</description>
			<name>type</name>
			<required>true</required>
			<deferred-value>
				<type>java.lang.String</type>
			</deferred-value>
		</attribute>
		<attribute>
			<description>Name of the exported file</description>
			<name>fileName</name>
			<required>true</required>
			<deferred-value>
				<type>java.lang.String</type>
			</deferred-value>
		</attribute>
		<attribute>
			<description>Comma seperated list of columns to be neglected</description>
			<name>excludeColumns</name>
			<required>false</required>
			<deferred-value>
				<type>java.lang.String</type>
			</deferred-value>
		</attribute>
		<attribute>
			<name>preProcessor</name>
			<required>false</required>
			<description><![CDATA[Pre hook to manipulate exported document.]]></description>
			<deferred-method>
				<method-signature>void preProcessor(java.lang.Object)</method-signature>
			</deferred-method>
		</attribute>
		<attribute>
			<name>postProcessor</name>
			<required>false</required>
			<description><![CDATA[Post hook to manipulate exported document.]]></description>
			<deferred-method>
				<method-signature>void postProcessor(java.lang.Object)</method-signature>
			</deferred-method>
		</attribute>
	</tag>
	
