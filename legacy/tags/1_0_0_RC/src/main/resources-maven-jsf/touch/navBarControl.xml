<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE component SYSTEM "../misc/component.dtd" [
<!ENTITY standard_uicomponent_attributes SYSTEM "../entities/standard_uicomponent_attributes.xml">
]>
<component>
	<tag>navBarControl</tag>
	<tagClass>org.primefaces.touch.component.navbarcontrol.NavBarControlTag</tagClass>
	<componentClass>org.primefaces.touch.component.navbarcontrol.NavBarControl</componentClass>
	<componentType>org.primefaces.touch.NavBarControl</componentType>
	<componentFamily>org.primefaces.touch</componentFamily>
	<parent>javax.faces.component.UIComponentBase</parent>
	<attributes>
		&standard_uicomponent_attributes;
		<attribute>
			<name>label</name>
			<required>false</required>
			<type>java.lang.String</type>
			<description>Label of the item.</description>
		</attribute>
		<attribute>
			<name>view</name>
			<required>false</required>
			<type>java.lang.String</type>
			<description>Id of the view to be displayed when this control is clicked.</description>
		</attribute>
		<attribute>
			<name>type</name>
			<required>false</required>
			<type>java.lang.String</type>
			<defaultValue>back</defaultValue>
			<description>Display type of the control, 'button' or 'back'.</description>
		</attribute>
		<attribute>
			<name>effect</name>
			<required>false</required>
			<type>java.lang.String</type>
			<description>Effect of the animation that'll be used when displaying the new view.</description>
		</attribute>
	</attributes>
</component>