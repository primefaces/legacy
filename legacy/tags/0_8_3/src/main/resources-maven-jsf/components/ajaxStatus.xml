<!DOCTYPE component SYSTEM "../misc/component.dtd" [
<!ENTITY standard_uicomponent_attributes		SYSTEM "../entities/standard_uicomponent_attributes.xml">
]>
<component>
	<tag>ajaxStatus</tag>
	<tagClass>org.primefaces.ui.component.ajaxstatus.AjaxStatusTag</tagClass>
	<componentClass>org.primefaces.ui.component.ajaxstatus.AjaxStatus</componentClass>
	<componentType>org.primefaces.ui.AjaxStatus</componentType>
	<componentFamily>org.primefaces.ui</componentFamily>
	<rendererType>org.primefaces.ui.component.AjaxStatusRenderer</rendererType>
	<rendererClass>org.primefaces.ui.component.ajaxstatus.AjaxStatusRenderer</rendererClass>
	<parent>javax.faces.component.UIComponentBase</parent>
	<attributes>
		&standard_uicomponent_attributes;
		<attribute>
			<name>onstart</name>
			<required>false</required>
			<type>java.lang.String</type>
			<description>
				Javascript event to be called when ajax requests are created.
			</description>
		</attribute>
		<attribute>
			<name>oncomplete</name>
			<required>false</required>
			<type>java.lang.String</type>
			<description>
				Javascript event to be called when ajax requests are created.
			</description>
		</attribute>
	</attributes>
	<resources>
		<resource>
			<name>/prototype/prototype.js</name>
		</resource>
	</resources>
</component>