<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE component SYSTEM "../misc/component.dtd" [
<!ENTITY standard_uicomponent_attributes		SYSTEM "../entities/standard_uicomponent_attributes.xml">
<!ENTITY output_component_attributes			SYSTEM "../entities/output_component_attributes.xml">
<!ENTITY input_component_attributes				SYSTEM "../entities/input_component_attributes.xml">
<!ENTITY command_component_attributes			SYSTEM "../entities/command_component_attributes.xml">
<!ENTITY html_input_text_attributes				SYSTEM "../entities/html_input_text_attributes.xml">
]>
<component>
	<tag>graphicText</tag>
	<tagClass>org.primefaces.component.graphictext.GraphicTextTag</tagClass>
	<componentClass>org.primefaces.component.graphictext.GraphicText</componentClass>
	<componentType>org.primefaces.component.graphictext</componentType>
	<componentFamily>org.primefaces.component</componentFamily>
	<rendererType>org.primefaces.component.graphictext.GraphicTextRenderer</rendererType>
	<rendererClass>org.primefaces.component.graphictext.GraphicTextRenderer</rendererClass>
	<parent>javax.faces.component.html.HtmlGraphicImage</parent>
	<attributes>
		&standard_uicomponent_attributes;
		<attribute>
			<name>value</name>
			<required>false</required>
			<type>java.lang.Object</type>
			<ignoreInComponent>true</ignoreInComponent>
			<description>
				Text value that will be rendered as an image
			</description>
		</attribute>
		<attribute>
			<name>alt</name>
			<required>false</required>
			<type>java.lang.String</type>
			<ignoreInComponent>true</ignoreInComponent>
			<description>
				 Alternate text for the image
			</description>
		</attribute>		
		<attribute>
			<name>fontName</name>
			<required>true</required>
			<type>java.lang.String</type>
			<defaultValue>Verdana</defaultValue>
			<description>
				Name of the font.
			</description>
		</attribute>
		<attribute>
			<name>fontStyle</name>
			<required>false</required>
			<type>java.lang.String</type>
			<defaultValue>plain</defaultValue>
			<description>
				Style of the font. Values are bold, italic, plain
			</description>
		</attribute>
		<attribute>
			<name>fontSize</name>
			<required>false</required>
			<type>java.lang.Integer</type>
			<defaultValue>12</defaultValue>
			<description>
				Size of the font
			</description>
		</attribute> 
		<attribute>
			<name>title</name>
			<required>false</required>
			<type>java.lang.String</type>
			<ignoreInComponent>true</ignoreInComponent>
			<description>
				 Title of the image.
			</description>
		</attribute>
		<attribute>
			<name>style</name>
			<required>false</required>
			<type>java.lang.String</type>
			<ignoreInComponent>true</ignoreInComponent>
			<description>
				 Style of the image.
			</description>
		</attribute>
		<attribute>
			<name>styleClass</name>
			<required>false</required>
			<type>java.lang.String</type>
			<ignoreInComponent>true</ignoreInComponent>
			<description>
				 Style class of the image.
			</description>
		</attribute>
		<attribute>
			<name>onclick</name>
			<required>false</required>
			<type>java.lang.String</type>
			<ignoreInComponent>true</ignoreInComponent>
			<description>
				 onclick dom event handler
			</description>
		</attribute>
		<attribute>
			<name>ondblclick</name>
			<required>false</required>
			<type>java.lang.String</type>
			<ignoreInComponent>true</ignoreInComponent>
			<description>
				 ondblclick dom event handler
			</description>
		</attribute>
		<attribute>
			<name>onkeydown</name>
			<required>false</required>
			<type>java.lang.String</type>
			<ignoreInComponent>true</ignoreInComponent>
			<description>
				 onkeydown dom event handler
			</description>
		</attribute>
		<attribute>
			<name>onkeypress</name>
			<required>false</required>
			<type>java.lang.String</type>
			<ignoreInComponent>true</ignoreInComponent>
			<description>
				 onkeypress dom event handler
			</description>
		</attribute>
		<attribute>
			<name>onkeyup</name>
			<required>false</required>
			<type>java.lang.String</type>
			<ignoreInComponent>true</ignoreInComponent>
			<description>
				 onkeyup dom event handler
			</description>
		</attribute>
		<attribute>
			<name>onmousedown</name>
			<required>false</required>
			<type>java.lang.String</type>
			<ignoreInComponent>true</ignoreInComponent>
			<description>
				 onmousedown dom event handler
			</description>
		</attribute>
		<attribute>
			<name>onmousemove</name>
			<required>false</required>
			<type>java.lang.String</type>
			<ignoreInComponent>true</ignoreInComponent>
			<description>
				 onmousemove dom event handler
			</description>
		</attribute>
		<attribute>
			<name>onmouseout</name>
			<required>false</required>
			<type>java.lang.String</type>
			<ignoreInComponent>true</ignoreInComponent>
			<description>
				 onmouseout dom event handler
			</description>
		</attribute>
		<attribute>
			<name>onmouseover</name>
			<required>false</required>
			<type>java.lang.String</type>
			<ignoreInComponent>true</ignoreInComponent>
			<description>
				 onmouseover dom event handler
			</description>
		</attribute>
		<attribute>
			<name>onmouseup</name>
			<required>false</required>
			<type>java.lang.String</type>
			<ignoreInComponent>true</ignoreInComponent>
			<description>
				 onmouseup dom event handler
			</description>
		</attribute>
	</attributes>
</component>